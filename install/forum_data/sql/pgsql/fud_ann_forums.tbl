CREATE TABLE {SQL_TABLE_PREFIX}ann_forums
(
	id		SERIAL PRIMARY KEY,
	ann_id		INT NOT NULL,
	forum_id	INT NOT NULL
);

CREATE INDEX {SQL_TABLE_PREFIX}ann_forums_index_ann_id_forum_id ON {SQL_TABLE_PREFIX}ann_forums (ann_id, forum_id);
