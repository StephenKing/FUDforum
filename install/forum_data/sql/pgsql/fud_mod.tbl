CREATE TABLE {SQL_TABLE_PREFIX}mod
(
	id		SERIAL PRIMARY KEY,
	user_id		INT NOT NULL,
	forum_id	INT NOT NULL
);

CREATE UNIQUE INDEX {SQL_TABLE_PREFIX}mod_index_user_id_forum_id ON {SQL_TABLE_PREFIX}mod (user_id,forum_id);
