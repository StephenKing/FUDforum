DROP TABLE IF EXISTS {SQL_TABLE_PREFIX}geoip;

CREATE TABLE {SQL_TABLE_PREFIX}geoip
(
	ips	BIGINT NOT NULL,
	ipe	BIGINT NOT NULL,
	cc	CHAR(2),
	country	VARCHAR(50),
	PRIMARY KEY (ips, ipe)
);
